<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="motorcycle_registry_view__list_inherit_motorcycle_registry" model='ir.ui.view'>
        <field name="name">motorcycle.registry.view.inherit.motorcycle.registry</field>
        <field name="model">motorcycle.registry</field>
        <field name="inherit_id" ref="motorcycle_registry.motorcycle_registry_view_list"/>
        <field name="arch" type="xml">
        <xpath expr="//field[@name='current_mileage']" position="after">
            <field name="stock_lot_id"/>
            <field name="sale_order"/>
            
        </xpath>
        </field>
    </record>

</odoo>